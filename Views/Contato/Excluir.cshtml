@model ContatoModel
@{
    ViewData["Title"] = "Excluir Contato";
}


<div class="text-center">
    <h3>Realmente deseja apagar o contato(@Model.Nome)</h3>

    <a class="btn btn-primary role=button w-100" asp-controller="Contato" asp-action="Index"> Cancelar Exclusão</a>
    <a class= "btn btn-danger role=button w-100" asp-controller="Contato" asp-route-id="@Model.Id" asp-action="Apagar"> Confirmar Exclusão</a>
</div>
